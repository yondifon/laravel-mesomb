{
    "name": "malico/laravel-mesomb",
    "description": "A laravel wrapper on top of Mesomb Payment API",
    "license": "MIT",
    "authors": [
        {
            "name": "Malico",
            "email": "hi@malico.me"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "guzzlehttp/guzzle": "^6.5|^7.0|^8.0",
        "illuminate/database": "~5.7|^6.0|^7.0|^8.0",
        "illuminate/support": "~5.7|^6.0|^7.0|^8.0",
        "malico/mobile-cm-php": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Malico\\MeSomb\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Malico\\MeSomb\\MeSombServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.19",
        "nunomaduro/collision": "5.10.0",
        "orchestra/testbench": "^6.20"
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    }
}
